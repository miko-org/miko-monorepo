{
	"collection": "@nestjs/schematics",
	"sourceRoot": "apps/bot/src",
	"monorepo": true,
	"root": "apps/bot",
	"compilerOptions": {
		"webpack": true,
		"tsConfigPath": "apps/bot/tsconfig.app.json"
	},
	"projects": {
		"bot": {
			"type": "application",
			"root": "apps/bot",
			"entryFile": "main",
			"sourceRoot": "apps/bot/src",
			"compilerOptions": {
				"tsConfigPath": "apps/bot/tsconfig.app.json"
			}
		},
		"common": {
			"type": "library",
			"root": "libs/common",
			"entryFile": "index",
			"sourceRoot": "libs/common/src",
			"compilerOptions": {
				"tsConfigPath": "libs/common/tsconfig.lib.json"
			}
		},
		"api": {
			"type": "application",
			"root": "apps/api",
			"entryFile": "main",
			"sourceRoot": "apps/api/src",
			"compilerOptions": {
				"tsConfigPath": "apps/api/tsconfig.app.json"
			}
		},
		"communication": {
			"type": "library",
			"root": "libs/communication",
			"entryFile": "index",
			"sourceRoot": "libs/communication/src",
			"compilerOptions": {
				"tsConfigPath": "libs/communication/tsconfig.lib.json"
			}
		},
		"core": {
			"type": "library",
			"root": "libs/core",
			"entryFile": "index",
			"sourceRoot": "libs/core/src",
			"compilerOptions": {
				"tsConfigPath": "libs/core/tsconfig.lib.json"
			}
		},
		"scheduler": {
			"type": "library",
			"root": "libs/scheduler",
			"entryFile": "index",
			"sourceRoot": "libs/scheduler/src",
			"compilerOptions": {
				"tsConfigPath": "libs/scheduler/tsconfig.lib.json"
			}
		},
		"templates": {
			"type": "library",
			"root": "libs/templates",
			"entryFile": "index",
			"sourceRoot": "libs/templates/src",
			"compilerOptions": {
				"tsConfigPath": "libs/templates/tsconfig.lib.json"
			}
		}
	}
}
